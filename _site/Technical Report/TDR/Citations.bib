	Journal = {Ocean Engineering},
	Keywords = {MPC, line-of-sight guidance, path following},
	Pages = {239-295},
	Read = {1},
	Title = {Path following of underactuated marine surface vessels using line-of-sight based model predictive control},
	Volume = {37},
	Year = {2010},
